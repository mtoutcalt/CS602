<div>
  <div class="table-responsive">
    <table class="table table-condensed">
    <thead>
      <tr>
        <th>Order ID</th>
        <th>Order Date Created</th>
        <th>Order Number</th>
      </tr>
    </thead>
    <tbody>
      {{#each data}}
        <tr>
          <td>{{this.id}}</td>
          <td>{{this.created}}</td>
          <td>{{this.orderNumber}}</td>
        </tr>
        {{/each}}
    </tbody>
    </table>
  </div>
</div>
